#include <iostream>
#include <fstream>
#include <vector>

using namespace std;

int mesToNumber(string month){
    if(month == "Jan"){
        return 1;
    }
    if (month == "Feb") {
        return 2;
    }
    if (month == "Mar") {
        return 3;
    }
    if (month == "Apr") {
        return 4;
    }
    if (month == "May") {
        return 5;
    }
    if (month == "Jun") {
        return 6;
    }
    if (month == "Jul") {
        return 7;
    }
    if (month == "Aug") {
        return 8;
    }
    if (month == "Sep") {
        return 9;
    }
    if (month == "Oct") {
        return 10;
    }
    if (month == "Nov") {
        return 11;
    }
    if (month == "Dec") {
        return 12;
    }
    else{
        return -1; // Si el mes no es v√°lido
    }

}


void leer(string archivo, vector <int> &vector){
    ifstream file(archivo);
    int n;
    if(file.is_open()){
        cout << " hoila";
        string clave = "";
        string line;
        while(getline(file,line)){
            for(int i = 0; i < 3; i++){
                clave += line[i];
                
            }
            cout << clave << endl;
        }
       
    }
    else{
        cout << "Archivo inexistente";
    }
}





int main(){
    vector <int> vector;
    leer("bitacora.txt", vector);

    return 0;
}
